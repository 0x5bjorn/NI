/etc/init.d/networking restart

echo -e '\n\n\n' | ssh-keygen
pwd
cp /root/.ssh/id_rsa.pub /shared/pc4_key.pub
touch /root/.ssh/known_hosts

until ping -c1 192.168.1.2 &> /dev/null; do :;done

